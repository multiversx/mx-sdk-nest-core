{
  "name": "@multiversx/sdk-nestjs-cli",
  "version": "1.0.8",
  "description": "MultiversX configuration schema helper functions",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "bin": {
    "sdk-nestjs-cli": "./dist/cli.js",
    "mxnest": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "compile": "tsc -p tsconfig.json",
    "prepare": "npm run compile",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "commander": "^12.0.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^14.0.0",
    "tslib": "^2.6.2",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  },
  "_moduleAliases": {
    "@mvx-monorepo/common": "dist/libs/common"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
